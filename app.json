{
  "expo": {
    "name": "CrimeDoor",
    "slug": "crimedoor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "crimedoor",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.masterk-web3dao.crimedoor",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.saadahmedkhanzada.crimeDoorTailwindConfig"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-build-properties"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1bc8bdc0-1018-4d8c-81ad-cace574eedc6"
      }
    },
    "packagerOpts": {
      "sourceExts": ["js", "jsx", "ts", "tsx"]
    }
  }
}
